;
; Test soot.coffi's translations of those bytecode instructions
; which act either on a single 64-bit stack word (described as
; a value of a "category 2 computational type" in the VM spec)
; or on a pair of 32-bit stack words (a value of a "category 1
; computational type").
;
; The instructions are
;   dup_x2 pop2 dup2 dup2_x1 dup2_x2
;
; This test was originally motivated by a typing bug reported by
; Rohan Hart <rohan.hart@peace.com> on 26 June 2003.  His example
; makes up the body of testCaseHart.

.class Cat1Cat2
.super java/lang/Object

.method public <init>()V
    .limit stack 1
    .limit locals 1
    aload_0
    invokenonvirtual java/lang/Object/<init>()V
    return
.end method

.method public static testCaseHart()Ljava/lang/Long;
    .limit stack 5
    .limit locals 0
    invokestatic Cat1Cat2/f()J
    new java/lang/Long
    dup_x2
    dup_x2
    pop
    invokespecial java/lang/Long/<init>(J)V
    areturn
.end method

.method private static f()J
    .limit stack 2
    .limit locals 0
    lconst_1
    lreturn
.end method

.method private static g()F
    .limit stack 1
    .limit locals 0
    fconst_1
    freturn
.end method

.method public static test_dup_x2_cat1(FFI)V
    .limit stack 4
    .limit locals 3
    fload_0
    fload_1
    iload_2
    dup_x2
    invokestatic Cat1Cat2/printStack(IFFI)V
    return
.end method

.method public static test_dup_x2_cat2(JI)V
    .limit stack 4
    .limit locals 3
    lload_0
    iload_2
    dup_x2
    invokestatic Cat1Cat2/printStack(IJI)V
    return
.end method

.method public static test_pop2_cat1(IFF)V
    .limit stack 3
    .limit locals 3
    iload_0
    fload_1
    fload_2
    pop2
    invokestatic Cat1Cat2/printStack(I)V
    ; Now try calling another function to get the values 
    ; to be popped onto the stack, so Soot can't just 
    ; completely optimize away the load onto the stack.
    iload_0
    invokestatic Cat1Cat2/g()F
    invokestatic Cat1Cat2/g()F
    pop2
    invokestatic Cat1Cat2/printStack(I)V
    return
.end method

.method public static test_pop2_cat2(IJ)V
    .limit stack 3
    .limit locals 3
    iload_0
    lload_1
    pop2
    invokestatic Cat1Cat2/printStack(I)V
    ; Now try calling another function to get the values 
    ; to be popped onto the stack, so Soot can't just 
    ; completely optimize away the load onto the stack.
    iload_0
    invokestatic Cat1Cat2/f()J
    pop2
    invokestatic Cat1Cat2/printStack(I)V
    return
.end method

.method public static test_dup2_cat1(IFF)V
    .limit stack 5
    .limit locals 3
    iload_0
    fload_1
    fload_2
    dup2
    invokestatic Cat1Cat2/printStack(IFFFF)V
    return
.end method

.method public static test_dup2_cat2(ID)V
    .limit stack 5
    .limit locals 3
    iload_0
    dload_1
    dup2
    invokestatic Cat1Cat2/printStack(IDD)V
    return
.end method

.method public static test_dup2_x1_cat1(IFF)V
    .limit stack 5
    .limit locals 3
    iload_0
    fload_1
    fload_2
    dup2_x1
    invokestatic Cat1Cat2/printStack(FFIFF)V
    return
.end method

.method public static test_dup2_x1_cat2(ID)V
    .limit stack 5
    .limit locals 3
    iload_0
    dload_1
    dup2_x1
    invokestatic Cat1Cat2/printStack(DID)V
    return
.end method

.method public static test_dup2_x2_cat1cat1(IFFII)V
    .limit stack 7
    .limit locals 5
    iload_0
    fload_1
    fload_2
    iload_3
    iload 4
    dup2_x2
    invokestatic Cat1Cat2/printStack(IIIFFII)V
    return
.end method

.method public static test_dup2_x2_cat1cat2(IFFD)V
    .limit stack 7
    .limit locals 5
    iload_0
    fload_1
    fload_2
    dload_3
    dup2_x2
    invokestatic Cat1Cat2/printStack(IDFFD)V
    return
.end method

.method public static test_dup2_x2_cat2cat1(IJFF)V
    .limit stack 7
    .limit locals 5
    iload_0
    lload_1
    fload_3
    fload 4
    dup2_x2
    invokestatic Cat1Cat2/printStack(IFFJFF)V
    return
.end method

.method public static test_dup2_x2_cat2cat2(IJD)V
    .limit stack 7
    .limit locals 5
    iload_0
    lload_1
    dload_3
    dup2_x2
    invokestatic Cat1Cat2/printStack(IDJD)V
    return
.end method

.method public static printStack(IFFI)V
     .limit stack 2
     .limit locals 5
     getstatic java/lang/System/out Ljava/io/PrintStream;
     astore 4
     aload 4
     iload_0
     invokevirtual java/io/PrintStream.println(I)V
     aload 4
     fload_1
     invokevirtual java/io/PrintStream.println(F)V
     aload 4
     fload_2
     invokevirtual java/io/PrintStream.println(F)V
     aload 4
     iload_3
     invokevirtual java/io/PrintStream.println(I)V
     aload 4
     invokevirtual java/io/PrintStream.println()V
     return
.end method

.method public static printStack(IJI)V
     .limit stack 3
     .limit locals 5
     getstatic java/lang/System/out Ljava/io/PrintStream;
     astore 4
     aload 4
     iload_0
     invokevirtual java/io/PrintStream.println(I)V
     aload 4
     lload_1
     invokevirtual java/io/PrintStream.println(J)V
     aload 4
     iload_3
     invokevirtual java/io/PrintStream.println(I)V
     aload 4
     invokevirtual java/io/PrintStream.println()V
     return
.end method

.method public static printStack(I)V
     .limit stack 2
     .limit locals 2
     getstatic java/lang/System/out Ljava/io/PrintStream;
     astore_1
     aload_1
     iload_0
     invokevirtual java/io/PrintStream.println(I)V
     aload_1
     invokevirtual java/io/PrintStream.println()V
     return
.end method

.method public static printStack(IFFFF)V
     .limit stack 2
     .limit locals 6
     getstatic java/lang/System/out Ljava/io/PrintStream;
     astore 5
     aload 5
     iload_0
     invokevirtual java/io/PrintStream.println(I)V
     aload 5
     fload_1
     invokevirtual java/io/PrintStream.println(F)V
     aload 5
     fload_2
     invokevirtual java/io/PrintStream.println(F)V
     aload 5
     fload_3
     invokevirtual java/io/PrintStream.println(F)V
     aload 5
     fload 4
     invokevirtual java/io/PrintStream.println(F)V
     aload 5
     invokevirtual java/io/PrintStream.println()V
     return
.end method

.method public static printStack(IDD)V
     .limit stack 3
     .limit locals 6
     getstatic java/lang/System/out Ljava/io/PrintStream;
     astore 5
     aload 5
     iload_0
     invokevirtual java/io/PrintStream.println(I)V
     aload 5
     dload_1
     invokevirtual java/io/PrintStream.println(D)V
     aload 5
     dload_3
     invokevirtual java/io/PrintStream.println(D)V
     aload 5
     invokevirtual java/io/PrintStream.println()V
     return
.end method

.method public static printStack(FFIFF)V
     .limit stack 2
     .limit locals 6
     getstatic java/lang/System/out Ljava/io/PrintStream;
     astore 5
     aload 5
     fload_0
     invokevirtual java/io/PrintStream.println(F)V
     aload 5
     fload_1
     invokevirtual java/io/PrintStream.println(F)V
     aload 5
     iload_2
     invokevirtual java/io/PrintStream.println(I)V
     aload 5
     fload_3
     invokevirtual java/io/PrintStream.println(F)V
     aload 5
     fload 4
     invokevirtual java/io/PrintStream.println(F)V
     aload 5
     invokevirtual java/io/PrintStream.println()V
     return
.end method

.method public static printStack(DID)V
     .limit stack 3
     .limit locals 6
     getstatic java/lang/System/out Ljava/io/PrintStream;
     astore 5
     aload 5
     dload_0
     invokevirtual java/io/PrintStream.println(D)V
     aload 5
     iload_2
     invokevirtual java/io/PrintStream.println(I)V
     aload 5
     dload_3
     invokevirtual java/io/PrintStream.println(D)V
     aload 5
     invokevirtual java/io/PrintStream.println()V
     return
.end method

.method public static printStack(IIIFFII)V
     .limit stack 2
     .limit locals 8
     getstatic java/lang/System/out Ljava/io/PrintStream;
     astore 7
     aload 7
     iload_0
     invokevirtual java/io/PrintStream.println(I)V
     aload 7
     iload_1
     invokevirtual java/io/PrintStream.println(I)V
     aload 7
     iload_2
     invokevirtual java/io/PrintStream.println(I)V
     aload 7
     fload_3
     invokevirtual java/io/PrintStream.println(F)V
     aload 7
     fload 4
     invokevirtual java/io/PrintStream.println(F)V
     aload 7
     iload 5
     invokevirtual java/io/PrintStream.println(I)V
     aload 7
     iload 6
     invokevirtual java/io/PrintStream.println(I)V
     aload 7
     invokevirtual java/io/PrintStream.println()V
     return
.end method

.method public static printStack(IDFFD)V
     .limit stack 3
